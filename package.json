  "license": "Apache License, Version 2.0",
  "version": "0.1.0",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "node.js",
    "nodejs",
    "portfolio",
    "risk",
    "management",
    "mean",
    "variance",
    "efficient",
    "frontier",
    "global",
    "minimum"
  ],
  "dependencies": {
    "command-line-args": "^5.0.2",
    "csv-parser": "^2.2.0",
    "date-fns": "^1.30.1",
    "immutable": "^4.0.0-rc.12",
    "log4js": "^4.0.2",
    "material-design-lite": "^1.3.0",
    "numeric": "^1.2.6",
    "react": "^16.12.0",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.12.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.3.0",
    "rxjs": "6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "clean": "rimraf build/ build-cli/ coverage/",
    "test-no-watch": "react-scripts test --env=jsdom --all --no-watch",
    "eject": "react-scripts eject",
    "flow": "flow",
    "prettier-check": "prettier --check './src/**/*.js'",
    "prettier-write": "prettier --write './src/**/*.js'",
    "eslint": "eslint ./src",
    "test-all": "run-s --print-name prettier-check eslint flow test-no-watch",
    "build-cli": "babel ./src --out-dir ./build-cli --ignore './src/ui','./src/index.js','./src/App.js','./src/App.test.js'",
    "build-cli2": "babel ./src --out-dir ./build-cli --ignore './src/ui','./src/index.js','./src/App.js','./src/App.test.js' && j2m ./build-cli/cli/prm.js",
    "start-cli": "node --experimental-modules ./build-cli/cli/prm.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.116.1",
    "js-to-mjs": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "string-to-stream": "^3.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
